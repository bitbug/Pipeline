RewriteRule ^projects/([a-z0-9-]+)/activity/?$ controller/project/activity_c.php?slug=$1&filter=all [QSA,L]

### USERS ###

RewriteRule ^users/search/([a-z-]+)/?$ controller/site/userSearch.process.php?relationship=$1 [QSA,L]
RewriteRule ^users/([a-zA-Z0-9-]{1,20})/?$ controller/site/user_c.php?un=$1 [QSA,L]
RewriteRule ^users/([a-zA-Z0-9-]{1,20})/process/?$ controller/site/user.process.php?un=$1 [QSA,L]
RewriteRule ^users/([a-zA-Z0-9-]{1,20})/upload/?$ controller/site/userPicture.process.php [L]