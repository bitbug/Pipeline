RewriteRule ^projects/([a-z0-9-]+)/files/(\d+)/?$ controller/project/files_c.php?slug=$1&page=$2 [QSA,L]
RewriteRule ^projects/([a-z0-9-]+)/files/?$ controller/project/files_c.php?slug=$1 [QSA,L]

